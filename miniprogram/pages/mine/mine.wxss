#minebox {
  background: linear-gradient(to bottom, #f01548, #f01548 60%, #f01548 40%, #f01548 20%, #f01548 10%, white);
  padding-bottom: 100px;
}
#setting {
  position: fixed;
  top: 10px;
  right: 20px;
  width: 25px;
  height: 25px;
}

#avatarbox {
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding-top: 60px;
  padding-bottom: 10px;
}

#avatar {
  width: 64px;
  height: 64px;
  border-radius: 50%;
  box-shadow: 0 0 20px white;
}
#jmpbox {
  margin-top: 40px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
}

#jmpbox view {
  width: 300rpx;
  text-align: center;
  padding: 30rpx 10rpx 30rpx 10rpx;
  margin: 10rpx;
  border-radius: 20rpx;
  box-shadow: 0 0 5px white;
  color: white;
  font-size: 20px;
  font-weight: bold;
  border:2px solid white;
}
